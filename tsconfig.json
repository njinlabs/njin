{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "paths": {
      "@njin-types/*": ["./src/types/*"],
      "@modules/*": ["./src/modules/*"]
    },
    "moduleResolution": "nodenext",
    "module": "NodeNext",
    "resolveJsonModule": true
  }
}
